<%- include('../../components/open.ejs'); %>
<ul class="grid grid-cols-2 sm:grid-cols-3 ">
   <% data.forEach(function(image){ %>
     <%- include('./show', {image: image}); %>
   <% }); %>
</ul>
<script>
  async function ajaxFunction(ImageId) {
    if (!confirm('confirm')){
      return ;
    }
    await $.ajax({
      url: `/images/${ImageId}`,
      type: 'DELETE',
      success: (data)=>{
        console.log(data)
        location.reload()
      },
      error: (error)=>{
        console.log(error)
      }
    })
  }
</script>
<%- include('../../components/close.ejs'); %>